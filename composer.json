{
  "name": "d3/vanilla-job-queue",
  "description": "A php vanilla job queue with handler.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "php",
    "job-queue"
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "php-ds/php-ds": "^v1.3.0",

    "laminas/laminas-servicemanager": "^3.6",
    "laminas/laminas-config-aggregator": "^1.2",

    "ramsey/uuid": "^4.0.0",
    "robmorgan/phinx": "^0.12.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.1",
    "roave/security-advisories": "dev-master",
    "symfony/var-dumper": "^v5.2.6",
    "phpstan/phpstan": "^0.12.84",
    "phan/phan": "^4.0.3"
  },
  "autoload": {
    "psr-4": {
      "JobQueueHandler\\": "src/JobQueueHandler",
      "EventSystem\\": "src/EventSystem",
      "MiniCliRoutes\\": "src/MiniCliRoutes",
      "App\\": "src/App"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  }
}
